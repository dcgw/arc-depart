{
  "private": true,
  "name": "arc-depart",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/dcgw/arc-depart",
  "bugs": "https://github.com/dcgw/arc-depart/issues",
  "repository": "github:dcgw/arc-depart",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "webpack --env production",
    "commit": "cz",
    "fix": "tsc && eslint . --fix && prettier --write .",
    "postinstall": "husky install",
    "lint": "tsc && eslint . && prettier --check .",
    "prepack": "tsc && pinst --disable",
    "postpack": "pinst --enable",
    "start": "webpack serve --open",
    "test": "ava"
  },
  "engines": {
    "node": "^14 || ^16 || ^18 || >=19"
  },
  "dependencies": {
    "@types/domready": "1.0.1",
    "@types/webpack-env": "1.18.3",
    "domready": "1.0.8",
    "excalibur": "0.29.3",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@commitlint/cz-commitlint": "17.8.1",
    "@softwareventures/eslint-config": "7.1.2",
    "@softwareventures/precise-commits": "3.0.21",
    "@softwareventures/prettier-config": "4.0.0",
    "@softwareventures/semantic-release-config": "3.1.0",
    "@softwareventures/webpack-config": "5.1.0",
    "@types/node": "17.0.45",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "ava": "5.3.1",
    "commitizen": "4.3.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "39.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-sonarjs": "0.21.0",
    "husky": "8.0.3",
    "inquirer": "8.2.6",
    "pinst": "3.0.0",
    "prettier": "2.8.8",
    "semantic-release": "19.0.5",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "packageManager": "yarn@3.6.4"
}
